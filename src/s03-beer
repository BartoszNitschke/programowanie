#include <iostream>

using namespace std;


int main()
{
	for (int i = 99; i >= 1; i--)
	{
		cout << i << " bottles of beer on the wall, " << i << " bottles of beer" << endl;
		cout << "Take on down, pass it around" << endl;
	}

		cout << "No more bottles of beer on the wall, no more bottles of beer" << endl;
		cout << "Go to the store and buy some more" << endl << endl;

	for (int i = 99; i >= 1; i--)
	{
		cout << i << " bottles of beer on the wall, " << i << " bottles of beer" << endl;
		cout << "Take on down, pass it around" << endl;
	}

		cout << "No more bottles of beer on the wall, no more bottles of beer" << endl;
		cout << "Go to the store and buy some more" << endl;

	return 0;
}
